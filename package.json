{
  "name": "@ugrc/plss-review",
  "version": "1.0.13",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation concurrently 'wait-on http://127.0.0.1:4000 && ./scripts/seed.sh && pnpm run dev:vite' 'pnpm run dev:firebase' 'cd functions && pnpm run build:watch'",
    "dev:firebase": "cross-env NODE_ENV=development firebase emulators:start --import=.emulator-data --only auth,functions,firestore,storage,pubsub",
    "dev:firebase-state": "cross-env NODE_ENV=development concurrently 'firebase emulators:start --import=.emulator-data --export-on-exit=.emulator-data --only auth,functions,firestore,storage' 'pnpm run dev:vite'",
    "dev:vite": "vite",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "pnpm run dev",
    "start:ci": "vite --host",
    "test": "vitest --silent --coverage --ui --open --passWithNoTests",
    "test:ci": "firebase emulators:exec --only storage,firestore 'vitest run --coverage'"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-table": "^8.21.3",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/esri-theme-toggle": "^1.1.2",
    "@ugrc/utah-design-system": "^1.34.1",
    "@ugrc/utilities": "^3.1.1",
    "firebase": "^12.8.0",
    "ky": "^1.14.3",
    "lucide-react": "^0.563.0",
    "luxon": "^3.7.2",
    "react": "^19.2.4",
    "react-aria-components": "^1.14.0",
    "react-content-loader": "^7.1.2",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "react-hook-form": "^7.71.1",
    "react-router": "^7.13.0",
    "react-stately": "^3.43.0",
    "tailwind-merge": "2.6.1"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@tanstack/eslint-plugin-query": "^5.91.4",
    "@types/luxon": "^3.7.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "4.0.18",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "firebase-functions-test": "^3.4.1",
    "firebase-tools": "^15.5.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-router-devtools": "^6.2.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "esbuild",
      "protobufjs",
      "re2",
      "unrs-resolver"
    ],
    "overrides": {
      "on-headers@<1.1.0": ">=1.1.0",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "form-data@<2.5.4": ">=2.5.4",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
      "vite@>=7.1.0 <=7.1.10": ">=7.1.11",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "node-forge@<1.3.2": ">=1.3.2",
      "jws@=4.0.0": ">=4.0.1",
      "jws@<3.2.3": ">=3.2.3",
      "storybook@>=9.0.0 <9.1.17": ">=9.1.17",
      "tar@<=7.5.2": ">=7.5.3",
      "tar@<=7.5.3": ">=7.5.4",
      "lodash-es@>=4.0.0 <=4.17.22": ">=4.17.23",
      "lodash@>=4.0.0 <=4.17.22": ">=4.17.23",
      "tar@<7.5.7": ">=7.5.7",
      "diff@>=4.0.0 <4.0.4": ">=4.0.4",
      "fast-xml-parser@>=4.3.6 <=5.3.3": ">=5.3.4",
      "axios@<=1.13.4": ">=1.13.5",
      "qs@<6.14.1": ">=6.14.1"
    },
    "ignoredBuiltDependencies": [
      "@vaadin/vaadin-usage-statistics"
    ]
  }
}
